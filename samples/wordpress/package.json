{
  "name": "@wordpress/web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite dev --host",
    "dev:wp": "vite dev --host --no-open",
    "build": "vite build",
    "preview": "vite preview",
    "build:copy:twig": "copy-and-watch --clean \"src/templates/components/**/*.twig\" ../templates/components",
    "build:copy:css": "copy-and-watch --clean dist/css/**/*.* ../static/css",
    "build:copy:js": "copy-and-watch --clean dist/js/**/*.js ../static/js",
    "build:copy:img": "copy-and-watch --clean dist/assets/**/*.* ../static/img",
    "build:set:resources": "copy-and-watch --clean src/templates/partials/resources/build/*.twig ../templates/partials/resources",
    "build:wp": "npm-run-all --serial build build:set:resources build:copy:twig build:copy:css build:copy:js build:copy:img",
    "watch:set:resources": "copy-and-watch --clean src/templates/partials/resources/dev/*.twig ../templates/partials/resources",
    "watch:copy:twig": "copy-and-watch --watch \"src/templates/components/**/*.twig\" ../templates/components",
    "watch:reload": "livereload . -e 'twig'",
    "watch:wp": "npm-run-all build:wp --parallel dev:wp watch:set:resources watch:copy:twig watch:reload",
    "start": "npm run dev"
  },
  "dependencies": {},
  "devDependencies": {
    "@websolutespa/ws-vite": "0.0.10",
    "copy-and-watch": "^0.1.6",
    "eslint": "^8.46.0",
    "eslint-config-websolute": "*",
    "livereload": "^0.9.3",
    "npm-run-all": "^4.1.5"
  }
}
